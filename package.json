{
  "name": "node101-landing",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=node101-landing",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name=node101-landing --branch=preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier --write --ignore-pattern pnpm-lock.yaml .",
    "format": "prettier --write --ignore-pattern pnpm-lock.yaml ."
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-linter-helpers": "^1.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwind-csstree": "^0.1.4",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.49.0",
    "wrangler": "4.59.2"
  },
  "dependencies": {
    "@astrojs/cloudflare": "13.0.0-beta.0",
    "astro": "6.0.0-beta.1"
  }
}
